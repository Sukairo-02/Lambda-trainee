+const getAvgFromApis = (source) => {
+    const isPrice = new RegExp(/Price$/);
+    return source.reduce((p, e) => {
+        let accum = 0;
+        let amt = 0;
+        for (const [key, value] of Object.entries(e)) {
+            if (isPrice.test(key) && !isNaN(Number(value))) {
+                amt++;
+                accum += Number(value);
+            }
+        }
+        p.push({ symbol: e.symbol, price: accum / amt });
+        return p;
+    }, []);
+};
