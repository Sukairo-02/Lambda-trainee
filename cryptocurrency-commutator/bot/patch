diff --git a/cryptocurrency-commutator/bot/src/app.ts b/cryptocurrency-commutator/bot/src/app.ts
new file mode 100644
index 0000000..5818652
--- /dev/null
+++ b/cryptocurrency-commutator/bot/src/app.ts
